strip.text.y = element_text(size = size),
legend.text.align = 0,
strip.placement = "outside",
strip.background = element_blank(),
axis.title.y = element_text(margin = margin(0, 10, 0, 0)),
axis.title.x = element_text(margin = margin(10, 0, 0, 0)))
